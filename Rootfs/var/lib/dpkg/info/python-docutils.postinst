#!/bin/sh
set -e

if [ "$1" = configure ]
then
	for exe in /usr/share/docutils/scripts/python2/*
	do
		update-alternatives --install /usr/bin/${exe##*/} ${exe##*/} $exe 30
	done
fi


# Automatically added by dh_python2:
if which pycompile >/dev/null 2>&1; then
	pycompile -p python-docutils 
fi

